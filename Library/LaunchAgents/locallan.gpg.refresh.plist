<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>locallan.gpg.refresh</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/gpg</string>
        <string>--refresh-keys</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>StartCalendarInterval</key>
    <dict>
        <key>Minute</key>
        <integer>0</integer>
        <key>Hour</key>
        <integer>23</integer>
        <key>Day</key>
        <integer>0</integer>
    </dict>
</dict>
</plist>
